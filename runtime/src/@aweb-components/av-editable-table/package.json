{
  "author": {
    "name": "zhanghaixian@agree.com.cn"
  },
  "bundleDependencies": false,
  "deprecated": false,
  "description": "可编辑表格",
  "docs": {
    "belongTo": "aui",
    "icon": "iconfont icon-password",
    "type": "",
    "href": "av-editable-table",
    "author": "zhanghaixian@agree.com.cn",
    "version": 610000,
    "accept": "divCtn",
    "name": "可编辑表格",
    "option": [{
      "type": "array",
      "desp": "列定义",
      "name": "col",
      "appendNumber": 1,
      "hidden": false,
      "defaultValue": "",
      "valueArray": "",
      "replaceTemp": true,
      "attrInEachElement": [{
          "defaultValue": "A列",
          "type": "string_input",
          "desp": "列名",
          "name": "sTitle"
        },
        
        {
          "defaultValue": "",
          "valueArray": [
            "text",
            "numberic",
            "checkbox",
            "select",
            "date"
          ],
          "type": "string_select",
          "desp": "列类型",
          "name": "columnType",
          "despArray": [
            "文本",
            "数字",
            "复选框",
            "下拉框",
            "日期"
          ]
        },
        {
          "defaultValue": "apple,banana,orange",
          "require": {
            "columnType": [
              "select"
            ]
          },
          "type": "string_input",
          "desp": "下拉选项名",
          "name": "dropDownOption",
          "details": "表示该下拉列下拉选项，传入字符串数组"
        },
        {
          "defaultValue": "1,2,3",
          "require": {
            "columnType": [
              "select"
            ]
          },
          "valueArray": "",
          "type": "string_input",
          "desp": "下拉选项值",
          "name": "dropDownValue"
        },
        {
          "defaultValue": false,
          "type": "boolean",
          "desp": "是否只读",
          "name": "readonly",
          "details": "设置当前列是否只读"
        },
        {
          "defaultValue": "apple,banana",
          "require": {
            "hasReadOnly": true,
            "columnType": [
              "select"
            ]
          },
          "type": "string_input",
          "desp": "下拉选项中不可修改数据",
          "name": "cantMod",
          "details": "类定义如果是下拉框，且存在这些数据则该数据不可更改，数据间以英文逗号分割，单个数据末尾也要加逗号"
        },
        {
          "defaultValue": 30,
          "require": {
            "columnType": [
              "img"
            ]
          },
          "type": "string_input",
          "desp": "图片高度",
          "name": "imgHeight"
        },
        {
          "defaultValue": 30,
          "require": {
            "columnType": [
              "img"
            ]
          },
          "type": "string_input",
          "desp": "图片宽度",
          "name": "imgWidth"
        }
      ]
    }]
  },
